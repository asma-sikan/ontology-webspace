@prefix ind: <https://asma-sikan.github.io/ontology-webspace/HealthcareMonitoringIndividuals.ttl#> .
@prefix onto: <https://asma-sikan.github.io/ontology-webspace/HealthcareMonitoringOntology.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://asma-sikan.github.io/ontology-webspace/HealthcareMonitoringIndividuals.ttl>
      rdf:type owl:Ontology ;
      owl:imports <https://asma-sikan.github.io/ontology-webspace/HealthcareMonitoringOntology.ttl> .

#################################################################
#    Individuals
#################################################################

### Alice
ind:Alice rdf:type owl:NamedIndividual ,
                  onto:CriticalPatient ,
                  onto:Patient ;
          onto:hasNurse ind:Eva ;
          onto:hasPrimaryDoctor ind:Dr_Smith ;
          onto:locatedIn ind:cityClinic ;
          onto:usesDevice ind:hrMonitor1 ;
          onto:age 75 .

### Bob
ind:Bob rdf:type owl:NamedIndividual ,
                 onto:Patient ;
        onto:hasNurse ind:Eva ;
        onto:hasPrimaryDoctor ind:Dr_Lee ;
        onto:locatedIn ind:cityClinic ;
        onto:usesDevice ind:oxySensor1 ;
        onto:age 45 .

### Carol
ind:Carol rdf:type owl:NamedIndividual ,
                   onto:Patient ;
          onto:hasPrimaryDoctor ind:Dr_Smith ;
          onto:locatedIn ind:cityClinic ;
          onto:age 72 .

### Dr Lee
ind:Dr_Lee rdf:type owl:NamedIndividual ,
                    onto:Doctor .

### Dr Smith
ind:Dr_Smith rdf:type owl:NamedIndividual ,
                      onto:Doctor .

### Eva (Nurse)
ind:Eva rdf:type owl:NamedIndividual ,
                 onto:Nurse .

### City Clinic
ind:cityClinic rdf:type owl:NamedIndividual ,
                        onto:Clinic .

### Devices
ind:hrMonitor1 rdf:type owl:NamedIndividual ,
                        onto:HeartRateMonitor .

ind:oxySensor1 rdf:type owl:NamedIndividual ,
                        onto:OxygenSensor .

### Rooms
ind:room101 rdf:type owl:NamedIndividual ,
                     onto:Room .

ind:room102 rdf:type owl:NamedIndividual ,
                     onto:Room .

### Telemedicine Service
ind:telemedService1 rdf:type owl:NamedIndividual ,
                             onto:TelemedicineService .
