                                                               owl:onProperty :usesDevice ;
                                                               owl:someValuesFrom :MonitoringDevice
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf :Patient .


